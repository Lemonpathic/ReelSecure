{
    "manifest_version": 3,
    "name": "Reel Secure",
    "version": "1.0",
    "description": "Keeping you reel secure from phishing attacks!",
    "permissions": [
        "activeTab",
        "https://api.openai.com/"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}